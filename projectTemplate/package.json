{
  "name": "duxapp",
  "version": "1.0.0",
  "private": true,
  "description": "duxapp",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "duxapp rn clearBuildAssets && taro build --type rn",
    "build:rn-android": "duxapp rn clearBuildAssets android && taro build --type rn --platform android",
    "build:rn-ios": "duxapp rn clearBuildAssets ios && taro build --type rn --platform ios",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "taro build --type rn --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "native-components:h5": "taro build native-components --type h5",
    "postinstall": "patch-package",
    "clear:static": "duxapp project clearStatic",
    "start": "taro build --type rn --watch --reset-cache --qr",
    "codepush:init": "duxapp rn codepushInit android && duxapp rn codepushInit ios",
    "codepush:key": "duxapp rn codepushDeploymentKey android && duxapp rn codepushDeploymentKey ios",
    "android": "react-native run-android",
    "debug:android": "duxapp rn clearBuildAssets android && taro build --type rn --platform android --apps=base && duxapp rn appDependencies && cd android && ./gradlew installDebug && cd .. && duxapp rn appDependencies",
    "build:android": "duxapp rn clearBuildAssets android && duxapp rn appDependencies && duxapp runtime enterFile && cd android && ./gradlew assembleRelease && cd .. && duxapp file copy android/app/build/outputs/apk/release/app-arm64-v8a-release.apk dist/build/app-arm64-v8a-release.apk",
    "pgyer:android": "duxapp rn pgyer dist/build/app-arm64-v8a-release.apk",
    "install:android": "adb install android/app/build/outputs/apk/release/app-arm64-v8a-release.apk",
    "codepush:android": "duxapp runtime enterFile && duxapp rn codepushRN android && cd .",
    "codepush:android-test": "duxapp runtime enterFile && duxapp rn codepushRN android Test && cd .",
    "pod": "duxapp rn appDependencies && pod-install && duxapp rn appDependencies",
    "ios": "react-native run-ios",
    "build:ios": "duxapp ios build",
    "export:ios": "duxapp ios export",
    "pgyer:ios": "duxapp rn pgyer dist/ios/duxapp.ipa/duxapp.ipa",
    "upload-ios": "duxapp ios upload dist/ios/duxapp.ipa/duxapp.ipa",
    "codepush:ios": "duxapp runtime enterFile && duxapp rn codepushRN ios && cd .",
    "codepush:ios-test": "duxapp runtime enterFile && duxapp rn codepushRN ios Test && cd .",
    "codepush": "duxapp runtime enterFile && duxapp rn codepushRN android && duxapp runtime enterFile && duxapp rn codepushRN ios && cd .",
    "codepush-test": "duxapp runtime enterFile && duxapp rn codepushRN android Test && duxapp runtime enterFile && duxapp rn codepushRN ios Test && cd .",
    "logo": "duxapp rn appIcon && react-native generate-bootsplash logo.png --background-color=FFFFFF --logo-width=100 --assets-path=asset --flavor=main",
    "test": "jest",
    "lint": "eslint ."
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
